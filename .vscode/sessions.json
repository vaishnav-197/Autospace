{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "main",
        "autoExecuteCommands": true,
        "icon": "person",
        "color": "terminal.ansiGreen",
        "commands": [
          ""
        ]
      },
      [
        {
          "name": "apps:api",
          "commands": [
            "cd apps/api",
            "npm run start:dev"
          ]
        },
        {
          "name": "apps:web",
          "commands": [
            "cd apps/web",
            "npm run dev"
          ]
        }
      ],
      [
        {
          "name": "Lib:sample-build",
          "commands": [
            "cd libs/sample-lib",
            "npm run build"
          ]
        }
      ]
    ],
    "saved-session": [
      {
        "name": "connect",
        "commands": [
          ""
        ]
      }
    ]
  }
}